NLSTAGUTIL = $(CDESRC)/localized/util/merge

.SUFFIXES: .tmsg .nls

.nls:
	@echo "Extracting NLS messages for: $*"
	$(NLSTAGUTIL) -lang $(LANG) $*.tmsg < $*.nls > $* || $(RM) $*
	$(RM) .*.m

