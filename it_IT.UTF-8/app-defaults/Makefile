       LDPOSTLIBS = -L$(USRLIBDIR)   -L$(XPROJECTROOT)/lib
     TOP_INCLUDES = -I$(TOP) $(TOP_CDE_INCLUDES) $(TOP_MOTIF_INCLUDES) $(TOP_X_INCLUDES)
  PROJECT_DEFINES =    -DANSICPP -DMULTIBYTE -DNLS16 	-DOSMAJORVERSION=2 -DOSMINORVERSION=6

