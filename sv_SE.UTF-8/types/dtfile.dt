ACTION DtfileHome
{
        LABEL           	Filhanterare - Hem
        ARG_COUNT		0
        ICON			Fphome
        TYPE            	TT_MSG
        TT_CLASS		TT_REQUEST
        TT_SCOPE		TT_SESSION
        TT_OPERATION		DtHome_Show
        DESCRIPTION     	Filhanterarfunktionen Hem (DtfileHome) öppnar ett \
                        filhanterarfönster för användarens hemmapp.
}

# #####

ACTION Dtfile
{
        LABEL           	Filhanterare
        ICON			Fphome
        TYPE            	TT_MSG
        TT_CLASS		TT_REQUEST
        TT_SCOPE		TT_SESSION
        TT_OPERATION		Edit
        TT_FILE		   	%Arg_1"Mapp som ska öppnas:"%
        TT_ARG0_MODE		TT_INOUT
        TT_ARG0_VTYPE   	FILE_NAME
        DESCRIPTION     	Filhanterarfunktionen (Dtfile) öppnar ett filhanterarfönster \
                        för den angivna mappen.
}

# #####

ACTION Dtappmgr
{
        LABEL           	Programhanterare
        ICON			Fpapps
        TYPE            	TT_MSG
        TT_CLASS		TT_REQUEST
        TT_SCOPE		TT_SESSION
        TT_OPERATION		DtTools_Show
        TT_FILE			/var/dt/appconfig/appmanager/$DTUSERSESSION
        TT_ARG0_MODE            TT_IN
        TT_ARG0_VTYPE           -title
        TT_ARG0_REP_TYPE        TT_REP_STRING
        TT_ARG0_VALUE   	Programhanterare
        TT_ARG1_MODE            TT_IN
        TT_ARG1_VTYPE           -help_volume
        TT_ARG1_REP_TYPE        TT_REP_STRING
        TT_ARG1_VALUE   	Appmanager
        DESCRIPTION     	Programhanterarfunktionen (Dtappmgr) öppnar ett \
                        filhanterarfönster för programhanteraren.
}

