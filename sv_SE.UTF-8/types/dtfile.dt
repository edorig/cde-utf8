ACTION PrintFolderList
{
        LABEL           SkrivUtMapplista
        TYPE            COMMAND
        WINDOW_TYPE     NO_STDIO
        EXEC_STRING     sh -c '                         ls -aLlF "%(File)Arg_1%" |                         /usr/dt/bin/dtlp -u "%(File)Arg_1%"'


        DESCRIPTION     Funktionen SkrivUtMapplista skriver ut innehållet \
                        i en mapp på standardskrivaren.
}

