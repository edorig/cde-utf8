ACTION PrintFolderList
{
        LABEL           Exscribere_contenitus_indicis
        TYPE            COMMAND
        WINDOW_TYPE     NO_STDIO
        EXEC_STRING     sh -c '                         ls -aLlF "%(File)Arg_1%" |                         /usr/dt/bin/dtlp -u "%(File)Arg_1%"'


        DESCRIPTION     L'action Exscribere_contenitus_indicis exscribet\
                        quot index continet cum machina pressoria.
}

